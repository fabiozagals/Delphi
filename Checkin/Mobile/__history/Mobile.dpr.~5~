program Mobile;

uses
  System.StartUpCopy,
  FMX.Forms,
  uPrincipal in 'uPrincipal.pas' {frmPrincipal},
  uFrmModelo in 'uFrmModelo.pas' {frmFrmModelo},
  uCheckin in 'uCheckin.pas' {frmCheckin},
  uCheckout in 'uCheckout.pas' {frmCheckout};

{$R *.res}

begin
  Application.Initialize;
  Application.CreateForm(TfrmPrincipal, frmPrincipal);
  Application.CreateForm(TfrmFrmModelo, frmFrmModelo);
  Application.CreateForm(TfrmCheckin, frmCheckin);
  Application.CreateForm(TfrmCheckout, frmCheckout);
  Application.Run;
end.
